<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.yesgaori.sns.comment.repository.CommentRepository">


<insert></insert>

<select id="selectCountLike" parameterType="int" resultType="map">

	SELECT
		number(1)
	FROME
		`likeCount`
	WHERE
		`postId` = ${postId }




</select>




</mapper>